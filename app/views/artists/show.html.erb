<h2><% if @artist.file_upload-file_name %>
      <%= image_tag @artist.file_upload.url(:thumb), class: "artist-thumb circle"%>
    <% end %><%= @artist.full_name %> </h2>

<div>
  <% @artist.items.each do |item| %>
    <%= render partial: "shared/item_card", locals: { item: item } %>
  <% end %>
</div>
